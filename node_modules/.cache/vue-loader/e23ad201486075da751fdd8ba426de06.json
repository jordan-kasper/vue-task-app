s: {\n    /**\n     * Adds task to tasks array\n     *\n     * @param {String} task the task passed from addTask component\n     */\n    addNewTask(task) {\n      this.tasks.push(task);\n    },\n    /**\n     * Removes all tasks from the tasks array\n     */\n    deleteAll() {\n      this.tasks = [];\n    },\n  },\n  components: {\n    appHeader: Header,\n    appAddTask: addTask,\n    appTaskList: TaskList,\n  },\n\n  mounted() {\n    if (localStorage.getItem('tasks')) {\n      this.tasks = JSON.parse(localStorage.getItem('tasks'));\n    }\n  },\n  watch: {\n    tasks: {\n      handler() {\n        localStorage.setItem('tasks', JSON.stringify(this.tasks));\n      },\n      deep: true,\n    },\n  },\n};\n</script>\n\n<style>\n.bd-placeholder-img {\n  font-size: 1.125rem;\n  text-anchor: middle;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  user-select: none;\n}\n\n@media (min-width: 768px) {\n  .bd-placeholder-img-lg {\n    font-size: 3.5rem;\n  }\n}\nbody {\n  padding-top: 5rem;\n}\n.starter-template {\n  padding: 3rem 1.5rem;\n  text-align: center;\n}\n.list-spacing {\n  margin-top: 100px;\n}\n</style>\n"]}]}