"sourcesContent":["<template>\n  <div id=\"app\">\n    <app-header></app-header>\n    <b-container>\n      <b-row>\n        <b-col>\n          <b-jumbotron\n            header=\"Vue Todo App\"\n            lead=\"A todo app built with Bootstrap v4 and Vue.js\"\n          >\n            <p>Add a task with a priority below</p>\n            <app-add-task id=\"add\" @taskAdded=\"addNewTask\"></app-add-task>\n          </b-jumbotron>\n        </b-col>\n        <b-col>\n          <app-show-tasks v-bind:tasks=\"tasks\"></app-show-tasks>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n\n<script>\nimport addTask from './components/addTask.vue';\nimport showTasks from './components/ShowTasks.vue';\nimport header from './components/header.vue';\n\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      tasks: [],\n    };\n  },\n  methods: {\n    /**\n     * Adds task to tasks array\n     *\n     * @param {String} task the task passed from addTask component\n     */\n    addNewTask(task) {\n      this.tasks.push(task);\n    },\n  },\n  components: {\n    appShowTasks: showTasks,\n    appAddTask: addTask,\n    appHeader: header,\n  },\n  mounted() {\n    if (localStorage.getItem('tasks')) {\n      this.tasks = JSON.parse(localStorage.getItem('tasks'));\n    }\n  },\n};\n</script>\n\n<style>\n.add {\n  margin-left: none;\n  padding-left: none;\n}\n.jumbotron {\n  background-color: white;\n}\n</style>\n"],"sourceRoot":"src"}]}