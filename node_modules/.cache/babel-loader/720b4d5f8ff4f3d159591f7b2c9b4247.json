= {\n        task: '',\n        priority: '',\n        selected: '',\n      };\n    },\n    validateBeforeSubmit() {\n      this.$validator.validateAll().then((result) => {\n        if (result) {\n          this.errors.clear();\n          this.submitTask();\n        }\n      });\n    },\n  },\n  components: {\n    BFormInput,\n    BForm,\n    BFormSelect,\n    BButton,\n  },\n};\n</script>\n\n<style scoped>\n.spacing {\n  margin-top: 20px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}